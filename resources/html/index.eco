<!doctype html>

<!-- Documentation generated by [CoffeeDoc](http://github.com/omarkhan/coffeedoc) -->

<html>
<head>
  <title>CoffeeDoc</title>
  <meta http-equiv="content-type" content="text/html; charset=UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <link rel="stylesheet" media="all" href="resources/base.css" />
</head>
<body>
  <header>
    <h1>CoffeeDoc &mdash; Module Index</h1>
  </header>
  <div class="container">
    <div class="sidebar column">
      <ul id="modulelist">
        <% for m in @modules: %>
        <li><a href="<%= m.filename %>.html"><%= m.filename %></a></li>
        <% end %>
      </ul>
    </div>
    <div class="content column">
      <% for m in @modules: %>
      <div class="module">
        <div class="header">
          <a href="<%= m.filename %>.html"><h1><%= m.module_name %></h1></a>
        </div>
        <div class="module-content">
          <% if m.module.docstring: %><%- m.module.docstring %><% end %>
          <% if m.module.classes.length > 0: %>
          <h3>Classes</h3>
          <ul class="classlist">
            <% for cls in m.module.classes: %>
            <li><a href="<%= m.filename %>.html#<%= cls.name %>"><%= cls.name %></a></li>
            <% end %>
          </ul>
          <% end %>
          <% if m.module.functions.length > 0: %>
          <h3>Functions</h3>
          <ul class="functionlist">
            <% for func in m.module.functions: %>
            <li><a href="<%= m.filename %>.html#<%= func.name %>"><%= func.name %>(<%= func.params.join(', ') %>)</a></li>
            <% end %>
          </ul>
          <% end %>
        </div>
      </div>
      <% end %>
    </div>
  </div>
</body>
</html>
